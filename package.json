{
  "name": "generator-tuba-react",
  "version": "1.0.21",
  "description":
    "React generator. Includes React, Express, Bootstrap4, and a lightly configured Webpack",
  "homepage": "http://github.com/amatranga/tuba-generator-react",
  "author": "amatranga <amatranga892@gmail.com> (github.com/amatranga)",
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "React",
    "Express",
    "Bootstrap4",
    "Webpack",
    "Tuba",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.6.0",
    "yeoman-assert": "^3.0.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "yeoman-generator": "^1.1.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amatranga/generator-tuba-react.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amatranga/generator-tuba-react/issues"
  }
}
